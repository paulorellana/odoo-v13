<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="inherit_stock_view_picking_form" model="ir.ui.view">
        <field name="name">inherit.stock.view.picking.form</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//button[@name='action_cancel']" position="before">
                <button name="action_open_guia_remision" string="Generar Guía de Remisión" class="oe_highlight" type="object"/>
            </xpath>
        </field>
    </record>
</odoo>
